rm -rf resogen0.wav
cargo run --bin sim --features for_host -- gen0.wav resogen0.wav 2>&1 | tee out
exit

rm -f out.wav tremsine.wav
cargo run --bin ramp --features for_host 2>&1 | tee out
cargo run --bin sim --features for_host -- sine.wav tremsine.wav 2>&1 | tee out
cargo run --bin graph --features for_host 2>&1 | tee out
cargo build --lib --features for_host 2>&1 | tee out

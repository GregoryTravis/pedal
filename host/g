rm -rf simout.wav
rm -rf out
./anal
cargo run --bin sim --features for_host -- simin.wav-input.wav /dev/null 2>&1 | tee -a out
jbe simin.wav-input.wav simin.wav-filtered.wav simin.wav-input-fft.wav simin.wav-filtered-fft.wav
exit

rm -rf simout.wav
cargo run --bin sim --features for_host -- simin.wav simout.wav 2>&1 | tee -a out
exit

rm -f stack-fft.wav
cargo run --bin fft --features for_host -- stack.wav stack-fft.wav 2>&1 | tee out
exit

cargo run --bin test --features for_host 2>&1 | tee out
exit

cargo test --lib --features for_host 2>&1 | tee out
exit

cargo run --bin graph --features for_host 2>&1 | tee out
exit

cargo run --bin gen_sine_table --features for_host 2>&1 | tee out
exit

rm -rf resogen0.wav
cargo run --bin sim --features for_host -- gen0.wav resogen0.wav 2>&1 | tee out
exit

rm -f out.wav tremsine.wav
cargo run --bin ramp --features for_host 2>&1 | tee out
cargo run --bin sim --features for_host -- sine.wav tremsine.wav 2>&1 | tee out
cargo build --lib --features for_host 2>&1 | tee out

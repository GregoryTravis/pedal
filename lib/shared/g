cargo run --bin edsl --features for_host 2>&1 | tee out
exit

cargo test --lib --features for_host 2>&1 | tee out
exit

cargo build --bin compare_wavs --features for_host 2>&1 | tee out
exit

cargo build --lib --features for_host 2>&1 | tee out
cargo build --lib --target thumbv7em-none-eabihf 2>&1 | tee out
cargo build --bin compare_wavs --features for_host 2>&1 | tee out
cargo build --bin dump --features for_host 2>&1 | tee out
cargo build --bin sine --features for_host 2>&1 | tee out
